#include <16F877A.H>
#fuses HS,NOWDT,NOLVP,NODEBUG,PUT,BROWNOUT 
#use delay (clock = 8000000)

void main(void) {
        //TRISA = 0;
        ADCON1 = 7;
        
        while (1) {
                output_high(PIN_A0);
                delay_ms(1000); // Delay 1s
                output_low(PIN_A0);
                delay_ms(1000); // Delay 1s

        }
}
